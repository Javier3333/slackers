    {
        "type": "message",
        "user": "U026NB2U2",
        "text": "many agencies do.",
        "ts": "1426093202.000008"
    },
    {
        "type": "message",
        "user": "U026E0ZNR",
        "text": "Not too surprised, just funny to me.",
