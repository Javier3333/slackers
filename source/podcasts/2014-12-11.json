                "thumb_url": "http:\/\/colbertnation.mtvnimages.com\/images\/shows\/colbert_report\/videos\/season11\/11035\/cr_11035_04.jpg?crop=true",
                "from_url": "http:\/\/thecolbertreport.cc.com\/guests\/sarah-koenig\/8i6klx\/sarah-koenig",
